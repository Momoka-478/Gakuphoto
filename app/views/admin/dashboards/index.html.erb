<h1>全ユーザー一覧</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to admin_user_path(user) do %>
        <%= user.name %>
      <% end %>
       - <%= user.email %>
      <%= link_to '退会', admin_user_path(user), method: :delete, data: { confirm: '本当に退会させますか？' } %> 
    </li>
  <% end %>
</ul>